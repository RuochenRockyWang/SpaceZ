<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="LaunchIcon" TargetType="{x:Type Image}">
        <Setter Property="Source" Value="../Images/launch.png"/>
    </Style>
    <Style x:Key="PayloadIcon" TargetType="{x:Type Image}">
        <Setter Property="Source" Value="../Images/payload.png"/>
    </Style>
</ResourceDictionary>